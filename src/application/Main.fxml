<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <GridPane fx:id="gpBase" gridLinesVisible="true" layoutX="14.0" layoutY="26.0" prefHeight="550.0" prefWidth="550.0">
        
      </GridPane>
      <Label fx:id="lblMain" layoutX="100.0" layoutY="200.0" prefHeight="100.0" prefWidth="600.0" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="cbSize" layoutX="340.0" layoutY="400.0" onAction="#comboSizeChanged" prefWidth="100.0" />
      <Button fx:id="btnHelp" layoutX="753.0" layoutY="26.0" mnemonicParsing="false" onAction="#showHelp" onMouseEntered="#showHelpTip" text="?">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <CheckBox fx:id="checkHints" layoutX="637.0" layoutY="37.0" mnemonicParsing="false" onAction="#handleCheckHints" prefWidth="100.0">
         <font>
            <Font size="18.0" />
         </font></CheckBox>
      
   </children>
</AnchorPane>
